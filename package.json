{
  "name": "xss-lsp-client",
  "displayName": "XSS LSP (Python AST)",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "XSS LSP",
      "properties": {
        "xssLsp.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Python executable used to run the LSP server"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.90.0"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
